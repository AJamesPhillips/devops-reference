  "homepage": "https://github.com/nanlabs/devops-reference#readme",
  "devDependencies": {
    "serverless": "^3.23.0",
    "serverless-offline": "^11.1.3",
    "serverless-python-requirements": "^5.4.0"
  },
  "dependencies": {
    "serverless-offline-sqs-external": "^1.0.6"
  }
}
