{
  "name": "nan-sqs-python",
  "version": "1.0.0",
  "description": "Serverless sqs with local stack and python example",
  "main": "index.js",
  "scripts": {
    "sls": "serverless",
    "sls:deploy": "serverless deploy",
    "sls:offline": "serverless offline start",
    "sls:package": "serverless package",
    "sls:invoke": "serverless invoke --function",
    "sls:invoke:local": "serverless invoke local --function",
    "test": "echo \"Error: no test specified\" && exit 1",
    "localstack-up": "docker-compose -f compose-localstack/docker-compose.yml up",
    "localstack-up-d": "docker-compose -f compose-localstack/docker-compose.yml up -d",
    "localstack-down": "docker-compose -f compose-localstack/docker-compose.yml down"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nanlabs/devops-reference.git"
